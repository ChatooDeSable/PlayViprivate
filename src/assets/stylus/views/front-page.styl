.front-page-container
  position: relative
  padding: 0 $page-margin

  .logo
    display: block
    margin-top: 20px
    margin-bottom: 65px
    height: 108px

  .big-text-button
    display: block
    margin-bottom: 20px

  .small-text-button
    position: absolute
    color: $white
    font-size: 1.8rem
    left: $page-margin
    top: calc(90vh - 100px)
